<section id="customer-story" class="customer-socialprint hidden">
	<div class="container">
		<div class="row">
			<div class="col-sm-5 col-md-6">
			</div>

			<div class="col-sm-7 col-md-5">
				<h2>Payroll made easy</h2>
				<div class="testimonial-quote">
					<q>I haven’t seen a government form since I switched to ZenPayroll and I used to drown in them from the other services…</q>
					<p class="quote-author"><span>- George Sylvain</span> CEO, Social Print Studio</p>
				</div>
				<p class="video-btn-wrapper">
					<a href="#" class="play-video-btn wistia-popover[height=422,playerColor=37823e,width=800]">George&#x27;s Story</a>
				</p>
			</div>
		</div>
	</div>
</section>



<section id="pricing" class="text-center top-divider pricing-standard ">
	<div class="container" style="min-height: 600px;">

    <div class="row">
		<div class="col-xs-12">
			<h2>Simple, tarif mensuel</h2>
		</div>
    </div>

    <div class="row text-center margin-top-30px">
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2 price-breakdown-wrapper">
			<div id="price-breakdown">
				<img alt="€25 + €5 par salarié" class="img-responsive" src="img/price-breakdown-sm-09b1d27c49f46d5e90e8b4b8cb23d822.png" />
				<p class="small-caps margin-top-15px">par mois</p>
			</div>
		</div>
    </div>

    <div class="row price-slider-wrapper">
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<span id="base">
				<noscript>
					€25 par société + €5 par salarié
				</noscript>
				<div id="zen-slider-url" class="hidden">
					<a href="#">Request Invite</a>
				</div>
				<div id="zen-slider-contact" class="hidden">
					<a href="#">Contact Us for Enterprise Pricing</a>
				</div>
				<div class="zen-slider"></div>
			</span>
		</div>
	</div>

	<div class="row">
		<div class="col-xs-12">
			<div id="total">
				<div class="row">
					<div class="total-text col-sm-10 col-sm-push-1 col-md-8 col-md-push-2">
						<p class="text-large">
							Avec
							<span class="text-color">1 salarié</span>
							dans votre société,
							<br>
							<span>€30 par mois</span>
							for delightful payroll
						</p>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-sm-8 col-sm-push-2">
			<p class="pricing-secondary-text">
				C'est tout! Tout pour la paye des salariés est incluse. 
				<span class="span-block-md">
					<strong>Pas de couts cachés</strong> ou additionnels.
				</span>
			</p>
		</div>
	</div>

    <div class="row">
		<div class="col-sm-8 col-sm-push-2">
			<p><a href="/fonctionnalites" class="paragraph-cta">Voir ce qui est inclu</a></p>
		</div>
    </div>

		<div class="row">
			<div class="col-sm-8 col-sm-push-2 pricing-signup dotted-top margin-top-30px">
				<form id="landing-signup-form" action="/inscription" method="GET">
					<div class="input-append">
						<button type="submit" class="btn primary">Essai gratuit</button>
					</div>
				</form>
				<h5>Gratuit pendant 2 mois</h5>
			</div>
		</div>
	</div>
</section>


<a class="anchor" id="pricing"></a>

<section id="pricing" class="pricing-2col hidden">
	<div class="container" style="min-height: 480px;">
		<div class="row">
			<div class="col-sm-6">
				<h2>Simple, monthly pricing</h2>
				<div class="price-breakdown-wrapper">
					<div id="price-breakdown">
						<img alt="$25 + $4 per person" class="img-responsive" src="img/price-breakdown-sm-09b1d27c49f46d5e90e8b4b8cb23d822.png" />
						<p class="small-caps margin-top-15px">per month</p>
					</div>
					<div class="dotted-hr"></div>
					<p>
						C'est tout! Everything for employees and contractors is included.
						<span class="span-block-md">
							<strong>Pas de couts cachés</strong> ou additionnels.
							<a href="/features" class="features-link">See what&#x27;s included</a>
						</span>
					</p>
					<form id="landing-signup-form" action="/inscription" method="GET">
						<div class="input-append">
							<button type="submit" class="btn primary">Essai gratuit</button>
						</div>
					</form>

					<h5>Free 2&#8209;month trial</h5>
				</div>
			</div>

			<div class="col-sm-6 calculate-price">
				<h3>How many people do you need to pay?</h3>
				<div class="calculate-price-wrapper">
					<div class="row price-slider-wrapper">
						<div class="col-sm-6 col-sm-offset-3 col-md-8 col-md-offset-2">
							<span id="base">
								<div id="zen-slider-url" class="hidden">
									<a href="#">Request Invite</a>
								</div>
								<div id="zen-slider-contact" class="hidden">
									<a href="#">Contact Us for Enterprise Pricing</a>
								</div>
								<div class="zen-slider">
									<div class="slider ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all">
										<div class="ui-slider-range ui-widget-header ui-corner-all ui-slider-range-min" style="width: 0%;">
										</div>
										<span class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 0%;">
											<div class="tooltip">
												<div class="tooltip-header"></div>
												<div class="tooltip-content">
													<h3>1 person</h3>
													<ul>
														<li>$25 base price</li>
														<li>$4/person x 1 person</li>
													</ul>
												</div>
											</div>
										</span>
									</div>
								</div>
							</span>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>


<% include /layouts/footer %>


</body>
</html>
